{
    "compilerOptions": {
        "module": "system",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "wwwroot/js/",
        "sourceMap": false,
        "target": "es3",
        "skipLibCheck": true,
        "listFiles": true,
        "alwaysStrict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strict": true
        
        
    },
    "include": [
        "ts/**/*"
    ]
}