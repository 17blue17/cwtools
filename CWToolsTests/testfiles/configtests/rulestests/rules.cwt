types = {
    type[ship_size] = {
        path = "game/common/ship_sizes"
    }
    type[name_from_file] = {
        path = "game/common/anomalies"
        name_from_file = yes
        subtype[subtype_one] = {
            subtype = yes
        }
    }
    type[event] = {
        path = "game/events"
    }
}
enums = {
enum[test_enum] = {
    my_enum
}
}

alias[effect:<ship_size>] = test
alias[effect:enum[test_enum]] = test
alias[effect:<name_from_file>] = test
alias[effect:<name_from_file.subtype_one>] = test2

event = {
    alias_name[effect] = alias_match_left[effect]
}