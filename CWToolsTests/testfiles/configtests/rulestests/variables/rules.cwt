types = {
    type[event] = {
        path = "game/events"
    }
    type[skipevent] = {
        path = "game/common/anomalies"
        skip_root_key = any
    }
}

event = {
    ## cardinality = 0..inf
    set_test = value_set[test]
    ## cardinality = 0..inf
    get_test = value[test]

    ## cardinality = 0..inf
    save_var = value_set[variable]
    ## cardinality = 0..inf
    scope_field = {
        test = test
    }

    ## cardinality = 0..inf
    use_variable = variable_field
}

skipevent = {
    ## cardinality = 0..inf
    set_test = value_set[skipevent]
    ## cardinality = 0..inf
    get_test = value[skipevent]

}

values = {
    value[variable] = {
        my_preset_variable
    }
}